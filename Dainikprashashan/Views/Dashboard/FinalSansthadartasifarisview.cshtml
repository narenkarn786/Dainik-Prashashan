                    </tr>
                </thead>

                <tbody>
                    <tr>
                        <td><a href="FinalSansthadartasifarisbiwaranview">@Html.DisplayFor(modelItem => Model.LastOrDefault().FormId)</a> </td>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Naam)</td>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Sabikthegana)</td>
                        
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Aabedan_miti)</td>
