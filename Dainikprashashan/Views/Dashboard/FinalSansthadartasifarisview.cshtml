    <script src="https://cdn.datatables.net/1.10.21/js/dataTables.bootstrap4.min.js"></script>

    <script>

        $(document).ready(function () {
            $('#example').DataTable();
        });
    </script>
</head>
<body>
    <div class="container" style="padding:15px 180px">
        <br />

        <a href="Sansthadartasifaris" style="color:white"><button type="button" class="btn btn-success"><h6><i class="fas fa-plus-circle"></i>&nbsp; नयाँ</h6></button> </a>

