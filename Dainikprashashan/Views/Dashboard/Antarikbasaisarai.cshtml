@model Dainikprashashan.Antarik_Basai_Sarai

@{
    ViewBag.Title = "Antarikbasaisarai";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<link href="http://nepalidatepicker.sajanmaharjan.com.np/nepali.datepicker/css/nepali.datepicker.v3.min.css" rel="stylesheet" type="text/css" />
<script src="http://nepalidatepicker.sajanmaharjan.com.np/nepali.datepicker/js/nepali.datepicker.v3.min.js" type="text/javascript"></script>
<script type="text/javascript">
    window.onload = function () {
        var mainInput = document.getElementById("nepali-datepicker");
        mainInput.nepaliDatePicker();
    };
</script>

<div style="padding:30px 200px;width:100%; margin:0 auto;">
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        <div class="col-sm-5">
            <label>आवेदन दिएको मिति</label>
            @Html.TextBoxFor(m => m.Aabedan_miti, "", new { @id = "nepali-datepicker", @name = "nepali-datepicker", @class = "form-control" })
            @Html.ValidationMessageFor(model => model.Aabedan_miti, "", new { @class = "text-danger" })
        </div>

        <br />

        <h4><b>आवेदकको विवरण</b></h4>
        <hr />
        <div class="row">
            <div class="col-sm-5">
                <label><b>नाम</b></label>

                @Html.EditorFor(model => model.Aabedak_naam, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Aabedak_naam, "", new { @class = "text-danger" })
            </div>


            <div class="col-sm-5">
                <label>
                    <b>
                        पुरानो स्थानमा बसोबास गर्न थालेको मिति
                    </b>
                </label>

                @Html.EditorFor(model => model.Purano_basobas_miti, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Purano_basobas_miti, "", new { @class = "text-danger" })
            </div>
        </div>
        <br />

        <div class="row">
            <div class="col-sm-5">
                <label><b>बुवाको नाम</b></label>

                @Html.EditorFor(model => model.Buwako_naam, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Buwako_naam, "", new { @class = "text-danger" })
            </div>
            <div class="col-sm-5">
                <label><b>आमाको नाम</b></label>

                @Html.EditorFor(model => model.Amako_naam, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Amako_naam, "", new { @class = "text-danger" })
            </div>
        </div>
        <br />


        <label><b>पुरानो ठेगाना  </b></label>
        <div class="row">
            <div class="col-sm-4">
                @Html.EditorFor(model => model.Purano_Pradesh, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Purano_Pradesh, "", new { @class = "text-danger" })
            </div>
            <div class="col-sm-4">
                @Html.EditorFor(model => model.Purano_Jilla, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Purano_Jilla, "", new { @class = "text-danger" })
            </div>
            <div class="col-sm-4">
                @Html.EditorFor(model => model.Purano_Gabisa, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Purano_Gabisa, "", new { @class = "text-danger" })
            </div>
        </div>
        <br />
        <div class="row">
            <div class="col-sm-6">
                @Html.EditorFor(model => model.Purano_Ward, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Purano_Ward, "", new { @class = "text-danger" })
            </div>
            <div class="col-sm-6">
                @Html.EditorFor(model => model.Purano_Tole, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Purano_Tole, "", new { @class = "text-danger" })
            </div>
        </div>
        <br />

        <label><b>हालको ठेगाना </b></label>
        <div class="row">
            <div class="col-sm-4">
                @Html.EditorFor(model => model.Haal_Pradesh, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Haal_Pradesh, "", new { @class = "text-danger" })
            </div>
            <div class="col-sm-4">
                @Html.EditorFor(model => model.Haal_Jilla, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Haal_Jilla, "", new { @class = "text-danger" })
            </div>
            <div class="col-sm-4">
                @Html.EditorFor(model => model.Haal_Gabisa, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Haal_Gabisa, "", new { @class = "text-danger" })
            </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-6">
                    @Html.EditorFor(model => model.Haal_Ward, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.Haal_Ward, "", new { @class = "text-danger" })
                </div>
                <div class="col-sm-6">
                    @Html.EditorFor(model => model.Haal_Tole, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.Haal_Tole, "", new { @class = "text-danger" })
                </div>
            </div>
            <br />
        <h4><b>परिवारको विवरण</b></h4>
        <hr />
        <table class="table-responsive table-bordered">
            <tr>
                <th>नाम</th>
                <th>नाता</th>
                <th>
                    ना.प्र.नं / जन्मदर्ता नं
                </th>
                <th>घर नं</th>
                <th>बाटोको नाम</th>
                <th>उमेर</th>
            </tr>
            <tr>
                <td>
                    @Html.EditorFor(model => model.Pariwaarko_naam, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.Pariwaarko_naam, "", new { @class = "text-danger" })
                </td>
                <td>
                    @Html.EditorFor(model => model.Nata, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.Nata, "", new { @class = "text-danger" })
                </td>
                <td>
                    @Html.EditorFor(model => model.Nagarikta_janmadarta_no, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.Nagarikta_janmadarta_no, "", new { @class = "text-danger" })
                </td>

                <td>
                    @Html.EditorFor(model => model.Ghar_no, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.Ghar_no, "", new { @class = "text-danger" })
                </td>
                <td>
                    @Html.EditorFor(model => model.Batoko_naam, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.Batoko_naam, "", new { @class = "text-danger" })
                </td>
                <td>
                    @Html.EditorFor(model => model.Umer, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.Umer, "", new { @class = "text-danger" })
                </td>
            </tr>
        </table>
        <div class="form-group">
            <button class="btn btn-success" type="submit">पेश गर्नुहोस् </button>
        </div>


    }
</div>
