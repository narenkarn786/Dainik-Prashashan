@model IEnumerable<Dainikprashashan.Models.GharkayamList>

@{
    ViewBag.Title = "Arthikbarsaview";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>
<br /><br />
<div class="row" style="padding:5px 200px">

    <div class="col-lg-10">
        <center> <h4><b>आर्थिक वर्षहरु</b></h4><hr /></center>
        <div class="table-responsive-sm">
            <table class="table table-striped table-bordered">

                <thead class="thead-dark">
                    <tr>
                        <th>
                            क्र.स.
                        </th>

                        <th>आर्थिक वर्ष</th>

                    </tr>
                </thead>

                <tbody>
                    @foreach (var m in Model)
                    {
                        <tr>
                            <td> @m.FormId </td>
                            <td>@m.Barsa</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
        <a href="Arthikbarsa"><button type="button" class="btn btn-success"><h6><i class="fas fa-plus-circle"></i>&nbsp; नयाँ</h6></button></a>
    </div>
</div>


