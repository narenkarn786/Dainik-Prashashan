@model IEnumerable<Dainikprashashan.Models.GharkayamList>
@{
    ViewBag.Title = "FinalByawasayadartaview";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1">

    <link href="https://cdn.datatables.net/1.10.15/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="https://cdn.datatables.net/responsive/2.1.1/css/responsive.bootstrap.min.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.css " rel="stylesheet" />
    <link href="https://cdn.datatables.net/1.10.21/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <script src="https://cdn.datatables.net/1.10.15/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.15/js/dataTables.bootstrap4.min.js "></script>
    <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
    <script src="https://cdn.datatables.net/1.10.21/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.21/js/dataTables.bootstrap4.min.js"></script>

    <script>

        $(document).ready(function () {
            $('#example').DataTable();
        });
    </script>
</head>
<body>
    <div class="container" style="padding:15px 180px">
        <div style="box-shadow: 0 0 20px 0 rgba(0, 0, 0, 0.2), 5px 5px 5px 0 rgba(0, 0, 0, 0.24);padding:20px 20px 20px 20px" ;>
            <h3><b>व्यवसाय दर्ता प्रमाणपत्र</b></h3>
            <hr />
            <div style="padding:10px 0px; width:100%; margin:0 auto;">
                <table id="example" class="table table-striped table-bordered" style="width:100%">
                    <thead class="thead-dark">
                        <tr>
                            <th>फारम ID</th>
                            <th>व्यवसायको नाम</th>
                            <th>दर्ता नं.	</th>
                            <th>प्यान नं.	</th>
                            <th>आवेदन गरिएको मिति</th>
                            <th>स्वीकृत</th>
                        </tr>
                    </thead>

                    <tbody>
                        <tr>
                            <td><a href="FinalByawasayadartabiwaranview">@Html.DisplayFor(modelItem => Model.LastOrDefault().FormId)</a> </td>
                            <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Byawasaya_naam)</td>
                            <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Darta_no)</td>
                            <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Pan_no)</td>
                            <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Aabedan_miti)</td>
                            <td>भएको</td>
                        </tr>




                    </tbody>
                </table>


                <hr />
                <a href="Byawasyadarta" style="color:white"><button type="button" class="btn btn-success"><h6><i class="fas fa-plus-circle"></i>&nbsp; नयाँ</h6></button> </a>

            </div>
        </div>
</body>
</html>


