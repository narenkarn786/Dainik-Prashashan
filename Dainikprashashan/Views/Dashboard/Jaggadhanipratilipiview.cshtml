@model IEnumerable<Dainikprashashan.Models.GharkayamList>
@{
    ViewBag.Title = "Jaggadhanipratilipiview";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row" style="padding:0px 100px">
    <div class="col-lg-1">
    </div>
    <center>
        <div class="col-lg-11">
            <div class="table-responsive-sm">
                <table class="table table-bordered" style="border:1px">

                    <tr>
                        <th scope="row">फारम ID</th>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().FormId)</td>
                    </tr>

                    <tr>
                        <th scope="row">
                            स्वीकृत
                        </th>
                        <td style="color:red">नभएको </td>
                    </tr>
                    <tr>आबदेकको  विवरण</tr>

                    <tr>
                        <th scope="row">नाम</th>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Aabedak_name)</td>
                    </tr>
                    <tr>
                        <th scope="row">ना.प्र.नं.	</th>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Nagariktano)</td>
                    </tr>
                    <tr>
                        <th scope="row">ना.प्र. जारी मिति	</th>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Nagariktamiti)</td>
                    </tr>
                    <tr>
                        <th scope="row">बुवाको नाम</th>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Buwakonaam)</td>
                    </tr>
                    <tr>
                        <th scope="row">बाजेको नाम	</th>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Bajekonaam)</td>
                    </tr>
                    <tr>
                        <th scope="row">ठेगाना</th>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Aabedakpradesh),@Html.DisplayFor(modelItem => Model.LastOrDefault().Aabedakjilla),@Html.DisplayFor(modelItem => Model.LastOrDefault().Aabedakgabisa),@Html.DisplayFor(modelItem => Model.LastOrDefault().Aabedakward)</td>
                    </tr>
                    <tr>
                        <th scope="row">साबिक ठेगाना		</th>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Aabedaksabikthegana)</td>
                    </tr>
                    <tr>
                        जग्गाको विवरण
                    </tr>
                    <tr>
                        <th scope="row">जग्गा धनिको नाम			</th>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Jaggadhanikonaam)</td>
                    </tr>
                    <tr>
                        <th scope="row">कित्ता नं.	</th>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Kitta_no)</td>
                    </tr>
                    <tr>
                        <th scope="row">क्षेत्रफल	</th>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Bigga),@Html.DisplayFor(modelItem => Model.LastOrDefault().Kattha),@Html.DisplayFor(modelItem => Model.LastOrDefault().Dhur)</td>
                    </tr>
                    <tr>
                        <th scope="row">ठेगाना</th>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Jaggapradesh),@Html.DisplayFor(modelItem => Model.LastOrDefault().Jaggajilla),@Html.DisplayFor(modelItem => Model.LastOrDefault().Jaggagabisa),@Html.DisplayFor(modelItem => Model.LastOrDefault().Jaggaward)</td>
                    </tr>
                    <tr>
                        <th scope="row">साबिक ठेगाना		</th>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Jaggasabikthegana)</td>
                    </tr>
                    <br />
                    <a href="Editabedanniwaran"><button class="btn btn-warning col-lg-5" type="submit"><b>सम्पादन गर्नुहोस् </b></button></a>
                    &nbsp;
                    <button class="btn btn-success col-lg-5" type="submit"><b>दर्ता गर्नुहोस् </b></button>
                </table>
            </div>
        </div>
    </center>
</div>