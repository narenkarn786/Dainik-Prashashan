@model IEnumerable<Dainikprashashan.Models.GharkayamList>
@{
    ViewBag.Title = "FinalKittakatsifarisview";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="https://cdn.datatables.net/1.10.15/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="https://cdn.datatables.net/responsive/2.1.1/css/responsive.bootstrap.min.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.css " rel="stylesheet" />
    <link href="https://cdn.datatables.net/1.10.21/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <script src="https://cdn.datatables.net/1.10.15/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.15/js/dataTables.bootstrap4.min.js "></script>
    <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
    <script src="https://cdn.datatables.net/1.10.21/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.21/js/dataTables.bootstrap4.min.js"></script>

    <script>

        $(document).ready(function () {
            $('#example').DataTable();
        });
    </script>
</head>
<body>
    <div class="container" style="padding:15px 180px">
        <button type="button" class="btn btn-success"><a href="Kittakatsifaris" style="color:white"><h6><i class="fas fa-plus-circle"></i>&nbsp; नयाँ</h6> </a></button>

        <br />
        <div style="padding:10px 0px; width:100%; margin:0 auto;">
            <table id="example" class="table table-striped table-bordered" style="width:100%">
                <thead>
                    <tr>
                        <th>फारम ID</th>
                        <th>आवेदकको नाम</th>
                        <th>नक्सा सिट नं	</th>
                        <th>कित्ता नं	</th>
                        <th>आवेदन गरिएको मिति</th>
                        <th>स्वीकृत</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td><a href="FinalGharjagganaamsaribiwaranview">@Html.DisplayFor(modelItem => Model.LastOrDefault().FormId)</a></td>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Aabedak_name)</td>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Naksa_sheetno)</td>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Kitta_no)</td>
                        <td>@Html.DisplayFor(modelItem => Model.LastOrDefault().Aabedan_miti)</td>
                        <td>भएको</td>
                    </tr>
                </tbody>

            </table>
        </div>
    </div>

</body>
</html>


